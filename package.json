{
  "name": "acey-deucey-game-web",
  "version": "1.0.0",
  "description": "A web app for the backgammon variant acey deucey",
  "main": "./app/index.js",
  "scripts": {
    "start": "static -a 0.0.0.0",
    "test": "jshint -c .jshintrc app && npm run build",
    "build": "node-sass --include-path node_modules/foundation-apps/scss index.scss built.css && webpack --progress --colors",
    "build-styles-watch": "node-sass -w --include-path node_modules/foundation-apps/scss index.scss built.css",
    "build-js-watch": "webpack-dev-server --progress --colors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KatherineThompson/acey-deucey-game-web.git"
  },
  "keywords": [
    "game"
  ],
  "author": "Katherine Thompson ket29@cornell.edu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KatherineThompson/acey-deucey-game-web/issues"
  },
  "homepage": "https://github.com/KatherineThompson/acey-deucey-game-web#readme",
  "devDependencies": {
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "foundation-apps": "^1.2.0",
    "jshint": "^2.9.1",
    "node-sass": "^3.4.2",
    "node-static": "^0.7.7",
    "webpack": "^1.12.15",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "angular": "^1.5.3",
    "lodash": "^4.11.1"
  }
}
